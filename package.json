{
  "name": "vuelayers",
  "fullname": "VueLayers",
  "version": "0.1.0",
  "description": "Vue components to work with OpenLayers",
  "author": "Vladimir Vershinin <ghettovoice@gmail.com>",
  "main": "dist/cjs/index.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "demo": "node build/dev-server.js --conf=demo-dev",
    "demo-deploy": "gh-pages -d ./dist/demo -m 'Publish demo'",
    "build-demo": "rm -rf ./dist/demo && node build/build.js --conf=demo-prod",
    "build-comp-cjs": "cross-env NODE_ENV=production node build/build.js --conf=comp-cjs",
    "build-lib-cjs": "cross-env NODE_ENV=production node build/build.js --conf=lib-cjs",
    "build": "rm -rf ./dist/cjs && npm run build-lib-cjs && npm run build-comp-cjs",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghettovoice/vuelayers.git"
  },
  "keywords": [
    "vuelayers",
    "vue",
    "openlayers"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghettovoice/vuelayers/issues"
  },
  "homepage": "https://github.com/ghettovoice/vuelayers",
