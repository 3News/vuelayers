{
  "name": "vuelayers",
  "fullname": "VueLayers",
  "version": "0.6.2",
  "description": "Vue.js components to work with OpenLayers",
  "author": "Vladimir Vershinin <ghettovoice@gmail.com>",
  "main": "dist/vuelayers.cjs.js",
  "module": "dist/vuelayers.es.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "rollup": "node build/rollup.js",
    "umd": "webpack --config=build/webpack.umd.conf.js --progress",
    "umd-min": "cross-env NODE_ENV=production webpack --config=build/webpack.umd.conf.js --progress",
    "build": "rm -rf dist && npm run lint && npm run rollup",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "travis": "npm run travis-test && npm run coveralls",
    "travis-test": "npm run unit && npm run e2e -- --env travis",
    "coveralls": "cat test/unit/coverage/lcov.info | coveralls",
    "docs-start": "cd docs-hexo && npm start",
    "docs-build": "cd docs-hexo && rm -rf public && npm run build",
    "docs-deploy": "cd docs-hexo && npm start"
  },
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghettovoice/vuelayers.git"
  },
  "keywords": [
    "vuelayers",
    "vue",
    "vuejs",
    "vue components",
    "openlayers",
    "ol",
    "web maps",
    "vuejs maps"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghettovoice/vuelayers/issues"
  },
  "homepage": "https://github.com/ghettovoice/vuelayers",
  "dependencies": {
