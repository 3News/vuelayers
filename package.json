{
  "name": "vuelayers",
  "fullname": "VueLayers",
  "version": "0.7.2",
  "description": "Vue 2 components to work with OpenLayers",
  "author": "Vladimir Vershinin <ghettovoice@gmail.com>",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "unpkg": "lib/index.umd.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "rollup": "node build/rollup.js",
    "umd": "webpack --config=build/webpack.umd.conf.js --progress",
    "umd-min": "cross-env NODE_ENV=production webpack --config=build/webpack.umd.conf.js --progress",
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run lint && npm run rollup",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue ./",
    "lint-fix": "eslint --ext .js,.vue --fix ./",
    "travis": "npm run travis-test && npm run coveralls",
    "travis-test": "npm run unit && npm run e2e -- --env travis",
    "coveralls": "cat test/unit/coverage/lcov.info | coveralls",
    "docs-start": "cross-env PORT=8082 node build/dev-server.js --name docs",
    "docs-clean": "rm -rf dist-docs",
    "docs-build": "npm run docs-clean && cross-env NODE_ENV=production webpack --config=build/webpack.docs.prod.conf.js --progress",
    "docs-deploy": "gh-pages -d dist-docs -m 'update doc site' -r git@github.com:VueLayers/vuelayers.github.io.git -b master",
    "jsdoc-parse": "jsdoc -c .jsdoc.js -r -X > docs/jsdoc-index.json"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghettovoice/vuelayers.git"
  },
  "keywords": [
    "vuelayers",
    "vue",
    "vuejs",
    "vue2",
    "vuejs2",
    "vue plugins",
    "vue components",
    "openlayers",
    "ol",
    "maps",
    "mapping"
  ],
  "license": "MIT",
  "bugs": {
