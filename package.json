{
  "name": "vuelayers",
  "fullname": "VueLayers",
  "version": "0.0.1",
  "description": "Vue components to work with OpenLayers 3",
  "author": "Vladimir Vershinin <ghettovoice@gmail.com>",
  "main": "dist/vuelayers.bundle.js",
  "module": "src/index.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "build-min-all": "cross-env NODE_ENV=production node build/build.js --umd --min",
    "build-min-apart": "cross-env NODE_ENV=production node build/build.js --umd --apart --min",
    "build-min": "npm run build-min-all && npm run build-min-apart",
    "build-all": "cross-env NODE_ENV=production node build/build.js --umd",
    "build-apart": "cross-env NODE_ENV=production node build/build.js --umd --apart",
    "build": "rm -rf ./dist && npm run build-all && npm run build-apart && npm run build-min",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ghettovoice/vuelayers.git"
  },
  "keywords": [
    "vuelayers",
    "vue",
    "openlayer"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/ghettovoice/vuelayers/issues"
  },
  "homepage": "https://gitlab.com/ghettovoice/vuelayers",
  "dependencies": {
    "vue": "^2.1.10"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.4",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chromedriver": "^2.27.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "friendly-errors-webpack-plugin": "^1.3.1",
