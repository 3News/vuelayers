{
  "presets": [
    ["env", { "modules": false }],
    "stage-0"
  ],
  "plugins": [
    "transform-runtime"
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    },
    "production": {
      "plugins": ["lodash"]
    },
    "production-cjs": {
      "plugins": [
        "lodash",
        ["./modules/babel-plugin-merge-imports", {
          "pkg": "openlayers",
          "pkgVar": "__ol",
          "regex": "^ol(?:\/(.*))?$"
        }]
      ]
    }
  }
}
