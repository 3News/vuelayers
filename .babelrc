{
  "presets": [
    ["latest", {
      "es2015": {
        "modules": false
      }
    }],
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "lodash"
  ],
  "env": {
    "cjs": {
      "plugins": [
        [
          "./modules/babel-plugin-transform-nested-import",
          {
            "pkg": "openlayers",
            "pkgVar": "__ol__",
            "regex": "^ol(?:\/(.*))?$"
          }
        ]
      ]
    },
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
