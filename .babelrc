{
  "presets": [
    ["env", { "modules": false }],
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "lodash"
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    },
    "cjs": {
      "plugins": [
        ["./modules/babel-plugin-merge-imports", {
          "pkg": "openlayers",
          "pkgVar": "__ol",
          "regex": "^ol(?:\/(.*))?$"
        }]
      ]
    }
  }
}
