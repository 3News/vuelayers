{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIns": true
    }],
    "stage-0"
  ],
  "plugins": [
    "lodash",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    },
    "production": {
      "plugins": []
    },
    "production-cjs": {
      "plugins": [
        ["./modules/babel-plugin-merge-imports", {
          "pkg": "openlayers",
          "pkgVar": "__ol",
          "regex": "^ol(?:\/(.*))?$"
        }]
      ]
    }
  }
}
