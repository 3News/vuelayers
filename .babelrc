{
  "presets": [
    [ "env", {
      "debug": true,
      "modules": false,
      "targets": {
        "node": 6,
        "browsers": [ "last 2 versions" ]
      }
    } ],
    "stage-0"
  ],
  "plugins": [
    "lodash"
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    },
    "cjs": {
      "plugins": [
        [ "./modules/babel-plugin-merge-imports", {
          "pkg": "openlayers",
          "pkgVar": "__ol",
          "regex": "^ol(?:\/(.*))?$"
        } ]
      ]
    }
  }
}
