{
  "presets": [
    "latest",
    "stage-0"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-es5-property-mutators",
    "transform-flow-strip-types",
    "lodash"
  ],
  "env": {
    "dist": {
      "plugins": [
        [
          "./modules/babel-plugin-transform-nested-import",
          {
            "pkg": "openlayers",
            "pkgVar": "__ol__",
            "regex": "^ol(?:\/(.*))?$"
          }
        ]
      ]
    },
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
